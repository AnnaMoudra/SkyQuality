  <body>
    <header>
      <div class="logo" role="banner">
        <a n:href="Homepage:default">
          <img src="{$basePath}/images/layout/siteLogo.png" title="SkyQuality Project" alt="SkyQuality Project" class="logo__img" />
          <span class="logo__description">projekt mapování jasu noční oblohy</span>
        </a>
      </div>

      <nav role="navigation">
        <div class="nav--user">
          <ul>        
            {if $user->loggedIn}
              <li><a n:href="Personal:">Můj účet</a></li> 
              <li><a n:href="Observation:create">Přidat měření</a></li>
              <li><a n:href="Sign:out">Odhlásit se</a></li>
            {else}
               <li><a n:href="Sign:in">Přihlásit</a></li>
            {/if}
          </ul>
        </div>

        <div class="nav--main">
          <ul>
            <li><a n:href="Database:">Databáze</a></li>
            <li><a n:href="Info:">Metodika</a></li> 
            <li><a n:href="Project:">O projektu</a></li>
          </ul> 
        </div>
      </nav>

      <div class="cistic"></div>

      <div  n:foreach="$flashes as $flash" class="flash {$flash->type}">
         {$flash->message}
      </div>

      <div class="cistichead"></div>

    </header>

    <div role="main">
      {include content}
  	</div>

    {block footer}
	    <footer>
        <div class="footer" role="contentinfo">
	        ©SkyQuality <br />
	        <a n:href="Project:">kontakt</a>
